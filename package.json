{
  "name": "7s-ui-example",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "run-desktop": "electron build/desktop",
    "run-desktop-dev": "electron http://localhost:8080",
    "make-desktop-dev-test": "APP_ENV=test webpack-dev-server --config webpack-desktop.config.js",
    "make-desktop-dev": "APP_ENV=dev webpack-dev-server --config webpack-desktop.config.js",
    "make-desktop": "webpack --config webpack-desktop.config.js",
    "make-web-dev-test": "APP_ENV=test webpack-dev-server --config webpack-web.config.js --history-api-fallback",
    "make-web-dev": "APP_ENV=dev webpack-dev-server --config webpack-web.config.js --history-api-fallback",
    "make-web": "webpack --config webpack-web.config.js",
    "make-web-test": "APP_ENV=production-test webpack --config webpack-web.config.js",
    "analyze-web": "APP_ENV=production webpack --config webpack-web.config.js --json | webpack-bundle-size-analyzer",
    "make-assets": "webpack --config webpack-assets-production.config.js",
    "make-assets-dev": "webpack-dev-server --config webpack-assets.config.js",
    "test-web": "mocha-webpack -r jsdom-global/register --webpack-config webpack-web.config.js src/test/web/*.js",
    "test-desktop": "mocha-webpack -r jsdom-global/register --webpack-config webpack-desktop.config.js src/test/desktop/*.js"
  },
  "author": "",
  "license": "Proprietary",
  "dependencies": {
    "bulma": "^0.7.0",
    "material-design-icons": "^3.0.1",
    "node-sass": "^4.12.0",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.4",
    "electron": "^1.6.18",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.1",
    "jsdom": "^11.1.0",
    "jsdom-global": "^3.0.2",
    "json-loader": "^0.5.7",
    "mocha": "^3.5.0",
    "mocha-webpack": "^0.7.0",
    "purify-css": "^1.2.2",
    "purifycss-webpack": "^0.7.0",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.20.0",
    "yaml-loader": "^0.5.0"
  }
}
